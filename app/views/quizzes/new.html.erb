<div class="block">
  <div class="secondary-navigation">
    <%= render :partial => 'quizzes/navigation' %>
  </div>          
  <div class="content">            
    <h2 class="title"><%= t("web-app-theme.new", :default => "New")%>  Quiz</h2>
    <div class="inner">
      <%= form_for @quiz, :url => quizzes_path, :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
